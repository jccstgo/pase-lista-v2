{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-useless-escape": "off",
    "no-control-regex": "off",
    "no-case-declarations": "off"
  }
}
